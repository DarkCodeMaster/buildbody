.round-click{
    height: 60px;
    width: 60px;
    background-color: #fd7b7b;
    border-radius: 100%;
    position: fixed;
    bottom: 150rpx;
    right: 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 2;
    
}
.round-clicktext{
    font-size: 16px;
    color: #fff;
    text-align: center;
}

.cover{
  position: absolute;
  width: 100%;
  height: 300%;
  
  z-index: 9999;
}