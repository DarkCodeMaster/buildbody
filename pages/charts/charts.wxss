page {
    background: #fafafa;
    display: flex;
    align-items: center;
    flex-direction: column;
    padding-top: 30rpx;
}
.bannerImage {
    box-shadow: 1px 1px 20rpx 0 #e28986;
    margin-bottom: 10rpx;
    border-radius: 10rpx;
}

ec-canvas {
  width: 100%;
  height: 100%;
  
}

.container {
  position: absolute;
  width: 100%;
  height: 40%;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background: #ffff;
  
} 

